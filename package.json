{
  "name": "ai-email-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "server": "nodemon server/src/server.ts",
    "server:build": "tsc -p server/tsconfig.json",
    "server:prod": "node server/dist/server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "db:migrate": "node db/migrate.js",
    "db:test": "node db/test-connection.js",
    "docker:test": "./scripts/test-docker.sh",
    "test:mail:start": "docker-compose -f docker-compose.test.yml up -d --remove-orphans",
    "test:mail:stop": "docker-compose -f docker-compose.test.yml down",
    "test:mail:setup": "bash scripts/setup-test-accounts.sh",
    "test:mail:logs": "docker logs test-mailserver -f",
    "test:mail:reset": "npm run test:mail:stop && rm -rf test-mail-data && npm run test:mail:start && npm run test:mail:setup",
    "create-test-users": "node scripts/create-test-users.js",
    "validate-extraction": "cd server && npx tsx src/lib/validate-extraction.ts",
    "inject-test-emails": "cd server && npx tsx src/lib/inject-test-emails.ts",
    "demo:websocket": "cd server && npx tsx src/scripts/demo-websocket-processing.ts",
    "demo:pipeline": "cd server && npx tsx src/scripts/demo-highland-pipeline.ts",
    "seed": "node scripts/seed-test-data.js",
    "qdrant:up": "docker compose up -d qdrant",
    "qdrant:down": "docker compose stop qdrant",
    "qdrant:logs": "docker compose logs qdrant",
    "qdrant:reset": "docker compose down qdrant && rm -rf qdrant_storage && docker compose up -d qdrant",
    "seed:db": "node scripts/create-test-users.js",
    "seed:mail": "bash scripts/setup-test-accounts.sh",
    "test:vector": "cd server && npx tsx src/scripts/test-vector-services.ts",
    "test:vector:comprehensive": "cd server && npx tsx src/scripts/comprehensive-vector-test.ts",
    "test:pipeline": "cd server && npx tsx src/lib/pipeline/test-pipeline.ts",
    "test:pipeline:mock": "cd server && npx tsx src/lib/pipeline/test-pipeline-mock.ts",
    "test:prompt": "cd server && npx tsx src/lib/pipeline/test-prompt-formatter.ts",
    "test:templates": "cd server && npx tsx src/lib/pipeline/test-templates.ts",
    "test:qdrant": "npx tsx server/src/lib/pipeline/test-qdrant-connection.ts",
    "test:unit": "jest --config jest.config.unit.js",
    "test:integration": "jest --config jest.config.integration.js",
    "tone:load": "cd server && npx tsx src/lib/pipeline/test-data-loader.ts load",
    "tone:clear": "cd server && npx tsx src/lib/pipeline/test-data-loader.ts clear",
    "tone:reset": "cd server && npx tsx src/lib/pipeline/test-data-loader.ts reset",
    "tone:demo": "cd server && npx tsx src/lib/pipeline/tone-learning-orchestrator.ts",
    "tone:e2e": "cd server && npx tsx src/lib/pipeline/test-e2e-flow.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@noble/hashes": "^1.8.0",
    "@qdrant/js-client-rest": "^1.14.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/cookie": "^0.6.0",
    "@types/email-reply-parser": "^1.4.2",
    "@types/handlebars": "^4.0.40",
    "@types/highland": "^2.13.0",
    "@xenova/transformers": "^2.17.2",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.2.12",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "email-reply-parser": "^2.0.1",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "highland": "^2.13.5",
    "html-to-text": "^9.0.5",
    "imap": "^0.8.19",
    "kysely": "^0.28.2",
    "lucide-react": "^0.525.0",
    "mailparser": "^3.7.4",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "sonner": "^2.0.6",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/html-to-text": "^9.0.4",
    "@types/imap": "^0.8.42",
    "@types/jest": "^30.0.0",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^20.19.8",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3"
  }
}
