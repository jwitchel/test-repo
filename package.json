{
  "name": "ai-email-assistant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "server": "nodemon server/src/server.ts",
    "server:build": "tsc -p server/tsconfig.json",
    "server:prod": "node server/dist/server.js",
    "test": "jest",
    "test:unit": "jest --config jest.config.unit.js",
    "test:integration": "jest --config jest.config.integration.js",
    "test:coverage": "jest --coverage",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:reset": "docker compose down && docker compose up -d",
    "db:migrate": "node db/migrate.js",
    "db:seed": "node scripts/create-test-users.js",
    "demo:nlp": "cd server && node src/scripts/demos/demo-nlp.js",
    "demo:nlp:interactive": "cd server && node src/scripts/demos/demo-nlp-interactive.js",
    "demo:pipeline": "cd server && npx tsx src/scripts/demos/demo-highland-pipeline.ts",
    "demo:websocket": "cd server && npx tsx src/scripts/demos/demo-websocket-processing.ts",
    "tone:load": "cd server && npx tsx src/scripts/tools/test-data-loader.ts load",
    "tone:demo": "cd server && npx tsx src/lib/pipeline/tone-learning-orchestrator.ts",
    "tone:e2e": "cd server && npx tsx src/scripts/tests/test-e2e-flow.ts",
    "vector:test": "cd server && npx tsx src/scripts/tests/vector-comprehensive-test.ts",
    "qdrant:reset": "docker compose down qdrant && rm -rf qdrant_storage && docker compose up -d qdrant"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@noble/hashes": "^1.8.0",
    "@qdrant/js-client-rest": "^1.14.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@types/cookie": "^0.6.0",
    "@types/email-reply-parser": "^1.4.2",
    "@types/handlebars": "^4.0.40",
    "@types/highland": "^2.13.0",
    "@xenova/transformers": "^2.17.2",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.2.12",
    "chalk": "^4.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compromise": "^14.14.4",
    "compromise-sentences": "^0.3.0",
    "compromise-stats": "^0.1.1",
    "cookie": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "email-reply-parser": "^2.0.1",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "highland": "^2.13.5",
    "html-to-text": "^9.0.5",
    "imap": "^0.8.19",
    "kysely": "^0.28.2",
    "lucide-react": "^0.525.0",
    "mailparser": "^3.7.4",
    "next": "15.3.5",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.60.0",
    "sonner": "^2.0.6",
    "swr": "^2.3.4",
    "tailwind-merge": "^3.3.1",
    "wink-sentiment": "^5.0.2",
    "ws": "^8.18.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/html-to-text": "^9.0.4",
    "@types/imap": "^0.8.42",
    "@types/jest": "^30.0.0",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^20.19.8",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3"
  }
}
