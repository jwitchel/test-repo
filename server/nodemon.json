{
  "watch": ["server/src"],
  "ext": "ts,js",
  "ignore": ["server/src/**/*.spec.ts", "server/src/**/*.test.ts", "qdrant_storage", "qdrant_storage/**/*"],
  "exec": "ts-node -r tsconfig-paths/register --project server/tsconfig.json server/src/server.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "delay": 2500
}